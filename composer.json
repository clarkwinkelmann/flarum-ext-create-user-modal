{
    "name": "clarkwinkelmann/flarum-ext-create-user-modal",
    "description": "A modal that does the same as Sign Up, but without connecting",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": ">=0.1.0-beta.11 <0.1.0-beta.13"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\CreateUserModal\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Create User Modal",
            "icon": {
                "name": "fas fa-user-plus",
                "backgroundColor": "#d13e32",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/22608"
        }
    }
}
